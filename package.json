{
  "name": "thesmartwon.com",
  "private": true,
  "description": "An optimized Gatsby site",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:html build:css build:static",
    "build:html": "node scripts/build.js",
    "build:css": "node-sass src/index.scss > public/main.css",
    "build:static": "shx cp -r static/* public",
    "clean": "shx rm -rf public",
    "deploy": "gh-pages --dist public",
    "start": "webpack-dev-server --open",
    "serve": "http-server"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "preact-helmet": "^4.0.0-alpha-3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.2",
    "css-loader": "^3.1.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "0.9.0",
    "mime": "^2.4.4",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "preact-render-to-string": "^4.1.0",
    "prerender-loader": "^1.3.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thesmartwon/thesmartwon.com"
  }
}
