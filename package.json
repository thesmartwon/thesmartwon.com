{
  "name": "thesmartwon.com",
  "private": true,
  "description": "An optimized statically rendered Preact site",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp",
    "build:css": "gulp css",
    "build:html": "gulp render",
    "build:js": "gulp js",
    "build:static": "gulp copy",
    "clean": "shx rm -rf public posts/**/*.md.js",
    "deploy": "gh-pages --dist dist",
    "serve": "cd dist && python3 -m http.server",
    "start": "gulp start"
  },
  "dependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "browser-sync": "^2.0.0",
    "camelcase-css": "^2.0.1",
    "cross-env": "^7.0.0",
    "fs-extra": "^9.0.0",
    "gh-pages": "2.0.1",
    "gulp": "^4.0.0",
    "mdast-util-to-hast": "^6.0.1",
    "mime": "^2.0.0",
    "moment": "^2.0.0",
    "node-sass": "^4.0.0",
    "preact-render-to-string": "^5.0.0",
    "rehype-format": "^3.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-mdx": "^1.0.0",
    "remark-parse": "^8.0.0",
    "remark-parse-yaml": "0.0.3",
    "rollup": "^2.0.0",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^6.0.0",
    "shx": "^0.3.2",
    "style-to-object": "^0.2.3",
    "through2": "^4.0.0",
    "unified": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thesmartwon/thesmartwon.com"
  }
}
