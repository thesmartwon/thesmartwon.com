{
  "name": "thesmartwon.com",
  "private": true,
  "description": "An optimized Gatsby site",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:html build:css build:static",
    "build:css": "node-sass src/main.scss > public/main.css",
    "build:html": "node scripts/build.js",
    "watch:html": "node scripts/build.js --watch",
    "build:js": "rollup -c --environment NODE_ENV:production",
    "build:static": "shx cp -r static/* public",
    "clean": "shx rm -rf public",
    "deploy": "gh-pages --dist public",
    "start": "webpack-dev-server --open",
    "serve": "http-server"
  },
  "dependencies": {
    "preact": "^10.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "camelcase-css": "^2.0.1",
    "chokidar": "^3.0.2",
    "fs-extra": "^8.1.0",
    "gh-pages": "2.0.1",
    "http-server": "0.9.0",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "preact-render-to-string": "^5.0.6",
    "rehype-format": "^2.3.2",
    "rehype-raw": "^4.0.1",
    "rehype-stringify": "^6.0.0",
    "remark-frontmatter": "^1.3.2",
    "remark-mdx": "^1.1.5",
    "remark-parse": "^7.0.1",
    "remark-parse-yaml": "0.0.3",
    "remark-rehype": "^5.0.0",
    "rollup": "^1.19.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "shx": "^0.3.2",
    "style-to-object": "^0.2.3",
    "to-vfile": "^6.0.0",
    "typescript": "^3.5.3",
    "unified": "^8.3.2",
    "vfile-reporter": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thesmartwon/thesmartwon.com"
  }
}
